{
	"name": "oa-frontend",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"description": "01星球大项目-09OA项目。这里是OA系统后台前端代码。",
	"packageManager": "pnpm@9.15.0",
	"engines": {
		"node": ">=22.14.0"
	},
	"scripts": {
		"dev": "vite --configLoader=runner",
		"build": "vite build --mode production --configLoader=runner",
		"test": "vitest --ui --watch --api.port=4000",
		"preview": "vite preview",
		"notuse-preinstall": "npx only-allow pnpm && npm run corepack:pnpm",
		"corepack:pnpm": "corepack enable && corepack install",
		"commit": "git-cz",
		"git:fetch": "git fetch -p",
		"up-ruan-cat": "pnpm up '@ruan-cat/*@latest'",
		"up": "pnpm up '!@wangeditor/' '!@form-create/'",
		"lint:eslint": "eslint . --fix",
		"format": "prettier --write .",
		"type-check": "vue-tsc --build",
		"deploy-vercel": "node --import=tsx ./scripts/vercel-deploy-tool.ts --env-path=.env.production",
		"clear:cache": "rimraf -g '**/{dist,.turbo,.vercel,.cache,.temp}'",
		"clear:deps": "rimraf -g '**/{node_modules,package-lock.json,pnpm-lock.yaml}'",
		"rm:types": "node --import=tsx ./scripts/del-types.ts",
		"predev": "npm run rm:types"
	},
	"dependencies": {
		"@element-plus/icons-vue": "^2.3.1",
		"@form-create/designer": "^3.2.11",
		"@form-create/element-ui": "^3.2.24",
		"@icon-park/vue-next": "^1.4.2",
		"@logicflow/core": "^2.0.16",
		"@logicflow/extension": "^2.0.21",
		"@vueuse/core": "^13.4.0",
		"@vueuse/integrations": "^13.4.0",
		"@wangeditor/editor": "^5.1.23",
		"@wangeditor/editor-for-vue": "^5.1.12",
		"axios": "^1.10.0",
		"crypto-js": "^4.2.0",
		"dayjs": "^1.11.13",
		"echarts": "^5.6.0",
		"element-plus": "^2.10.2",
		"js-pinyin": "^0.2.7",
		"jsoneditor": "^10.2.0",
		"lodash-es": "^4.17.21",
		"pdfobject": "^2.3.1",
		"pinia": "^3.0.3",
		"pinia-plugin-persistedstate": "^4.3.0",
		"pinyin": "4.0.0",
		"pinyin-pro": "^3.26.0",
		"qs": "^6.14.0",
		"splitpanes": "^4.0.4",
		"unplugin-vue-markdown": "^28.3.1",
		"vue": "^3.5.17",
		"vue-cropper": "^0.6.5",
		"vue-echarts": "^7.0.3",
		"vue-plugin-hiprint": "^0.0.60",
		"vue-router": "^4.5.1",
		"vue-sonner": "^2.0.1",
		"xlsx": "^0.18.5"
	},
	"devDependencies": {
		"@antfu/eslint-config": "^4.16.1",
		"@antfu/utils": "^9.2.0",
		"@ruan-cat/commitlint-config": "^1.1.4",
		"@ruan-cat/domains": "^0.6.0",
		"@ruan-cat/utils": "^4.7.0",
		"@ruan-cat/vercel-deploy-tool": "^0.9.1",
		"@ruan-cat/vuepress-preset-config": "^0.1.31",
		"@types/eslint-config-prettier": "^6.11.3",
		"@types/lodash-es": "^4.17.12",
		"@types/node": "^24.0.3",
		"@types/qs": "^6.14.0",
		"@types/rollup-plugin-visualizer": "^5.0.3",
		"@types/splitpanes": "^2.2.6",
		"@vitejs/plugin-vue": "^6.0.0",
		"@vitest/ui": "^3.2.4",
		"commitizen": "^4.3.1",
		"consola": "^3.4.2",
		"cross-env": "^7.0.3",
		"cz-git": "^1.11.2",
		"del": "^8.0.0",
		"eslint": "^9.29.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-prettier": "^5.5.0",
		"eslint-plugin-vue": "^10.2.0",
		"jsdom": "^26.1.0",
		"prettier": "^3.6.0",
		"prettier-plugin-lint-md": "^1.0.1",
		"rollup-plugin-visualizer": "^6.0.3",
		"sass-embedded": "^1.89.2",
		"tsx": "^4.20.3",
		"type-plus": "^7.6.2",
		"typescript": "^5.8.3",
		"unplugin-auto-import": "^19.3.0",
		"unplugin-element-plus": "^0.10.0",
		"unplugin-icons": "^22.1.0",
		"unplugin-vue-components": "^28.7.0",
		"unplugin-vue-router": "^0.12.0",
		"vercel": "^44.1.0",
		"vite": "^7.0.0",
		"vite-plugin-autogeneration-import-file": "^3.0.0",
		"vite-plugin-html": "^3.2.2",
		"vite-plugin-ts-alias": "^0.1.1",
		"vite-plugin-vue-devtools": "^7.7.7",
		"vitest": "^3.2.4"
	}
}