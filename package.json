{
	"name": "oa-frontend",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"description": "01星球大项目-09OA项目。这里是OA系统后台前端代码。",
	"packageManager": "pnpm@10.14.0",
	"engines": {
		"node": ">=22.14.0"
	},
	"scripts": {
		"dev": "vite --configLoader=runner",
		"build": "vite build --mode production --configLoader=runner",
		"test": "vitest --ui --watch --api.port=4000",
		"up-taze": "pnpm up @ruan-cat/taze-config -L && npx taze -r",
		"preview": "vite preview",
		"notuse-preinstall": "npx only-allow pnpm && npm run corepack:pnpm",
		"corepack:pnpm": "corepack enable && corepack install",
		"commit": "git-cz",
		"lint:eslint": "eslint . --fix",
		"format": "prettier --experimental-cli --write .",
		"type-check": "vue-tsc --build",
		"deploy-vercel": "tsx ./scripts/vercel-deploy-tool.ts --env-path=.env.production",
		"clear:cache": "rimraf -g '**/{dist,.turbo,.vercel,.cache,.temp}'",
		"clear:deps": "rimraf -g '**/{node_modules,package-lock.json,pnpm-lock.yaml}'",
		"rm:types": "tsx ./scripts/del-types.ts",
		"git:fetch": "git fetch -p",
		"predev": "npm run rm:types"
	},
	"dependencies": {
		"@element-plus/icons-vue": "^2.3.2",
		"@form-create/designer": "^3.3.0",
		"@form-create/element-ui": "^3.2.28",
		"@icon-park/vue-next": "^1.4.2",
		"@logicflow/core": "^2.1.1",
		"@logicflow/extension": "^2.1.2",
		"@vueuse/core": "^13.7.0",
		"@vueuse/integrations": "^13.7.0",
		"@wangeditor/editor": "^5.1.23",
		"@wangeditor/editor-for-vue": "^5.1.12",
		"axios": "^1.11.0",
		"crypto-js": "^4.2.0",
		"dayjs": "^1.11.13",
		"echarts": "^5.6.0",
		"element-plus": "^2.10.7",
		"js-pinyin": "^0.2.7",
		"jsoneditor": "^10.3.0",
		"lodash-es": "^4.17.21",
		"pdfobject": "^2.3.1",
		"pinia": "^3.0.3",
		"pinia-plugin-persistedstate": "^4.5.0",
		"pinyin": "4.0.0",
		"pinyin-pro": "^3.27.0",
		"qs": "^6.14.0",
		"splitpanes": "^4.0.4",
		"unplugin-vue-markdown": "^29.1.0",
		"vue": "^3.5.18",
		"vue-cropper": "^0.6.5",
		"vue-echarts": "^7.0.3",
		"vue-plugin-hiprint": "^0.0.60",
		"vue-router": "^4.5.1",
		"vue-sonner": "^2.0.8",
		"xlsx": "^0.18.5"
	},
	"devDependencies": {
		"@antfu/eslint-config": "^4.19.0",
		"@antfu/utils": "^9.2.0",
		"@prettier/plugin-oxc": "^0.0.4",
		"@ruan-cat/commitlint-config": "^1.4.0",
		"@ruan-cat/domains": "^0.9.0",
		"@ruan-cat/taze-config": "^0.2.0",
		"@ruan-cat/utils": "^4.10.0",
		"@ruan-cat/vercel-deploy-tool": "^0.11.0",
		"@ruan-cat/vite-plugin-ts-alias": "^0.1.0",
		"@ruan-cat/vuepress-preset-config": "^0.1.40",
		"@types/eslint-config-prettier": "^6.11.3",
		"@types/lodash-es": "^4.17.12",
		"@types/node": "^24.3.0",
		"@types/qs": "^6.14.0",
		"@types/rollup-plugin-visualizer": "^5.0.3",
		"@types/splitpanes": "^2.2.6",
		"@vitejs/plugin-vue": "^6.0.1",
		"@vitest/ui": "^3.2.4",
		"commitizen": "^4.3.1",
		"consola": "^3.4.2",
		"cross-env": "^7.0.3",
		"cz-git": "^1.12.0",
		"del": "^8.0.0",
		"eslint": "^9.33.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-prettier": "^5.5.4",
		"eslint-plugin-vue": "^10.4.0",
		"jsdom": "^26.1.0",
		"prettier": "^3.6.2",
		"prettier-plugin-lint-md": "^1.0.1",
		"rollup-plugin-visualizer": "^6.0.3",
		"sass-embedded": "^1.90.0",
