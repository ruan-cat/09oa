{
	"name": "oa-frontend",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"description": "01星球大项目-09OA项目。这里是OA系统后台前端代码。",
	"packageManager": "pnpm@9.5.0",
	"engines": {
		"node": ">=20.15.0",
		"vscode": "^1.22.0",
		"pnpm": ">=9.5.0"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-git"
		}
	},
	"scripts": {
		"dev": "vite",
		"build": "vite build --mode production",
		"preview": "vite preview",
		"test:unit": "vitest",
		"notuse-preinstall": "npx only-allow pnpm && npm run corepack:pnpm",
		"corepack:pnpm": "corepack enable && corepack install",
		"commit": "git-cz",
		"git:fetch": "git fetch -p",
		"up-ruan-cat": "pnpm up @ruan-cat/*@latest",
		"lint:eslint": "eslint . --fix",
		"format": "prettier --write .",
		"deploy-vercel": "node --import=tsx ./scripts/vercel-deploy-tool.ts --env-path=.env.production",
		"rm:node_modules": "rimraf node_modules pnpm-lock.yaml",
		"rm:types": "node --import=tsx ./scripts/del-types.ts",
		"predev": "npm run rm:types"
	},
	"dependencies": {
		"@element-plus/icons-vue": "^2.3.1",
		"@form-create/designer": "^3.2.10",
		"@form-create/element-ui": "^3.2.18",
		"@icon-park/vue-next": "^1.4.2",
		"@logicflow/core": "^2.0.10",
		"@logicflow/extension": "^2.0.14",
		"@vueuse/core": "^11.3.0",
		"@vueuse/integrations": "^11.3.0",
		"@wangeditor/editor": "^5.1.23",
		"@wangeditor/editor-for-vue": "^5.1.12",
		"axios": "^1.7.9",
		"crypto-js": "^4.2.0",
		"dayjs": "^1.11.13",
		"echarts": "^5.6.0",
		"element-plus": "^2.9.3",
		"js-pinyin": "^0.2.7",
		"jsoneditor": "^10.1.2",
		"lodash-es": "^4.17.21",
		"oa-frontend": "file:",
		"pdfobject": "^2.3.0",
		"pinia": "^2.3.1",
		"pinia-plugin-persistedstate": "^4.2.0",
		"pinyin": "4.0.0-alpha.2",
		"pinyin-pro": "^3.26.0",
		"qs": "^6.14.0",
		"splitpanes": "^3.1.8",
		"unplugin-vue-markdown": "^0.26.3",
		"vue": "^3.5.13",
		"vue-cropper": "^1.1.4",
		"vue-echarts": "^7.0.3",
		"vue-plugin-hiprint": "^0.0.56",
		"vue-router": "^4.5.0",
		"vue-sonner": "^1.3.0",
		"xlsx": "https://cdn.sheetjs.com/xlsx-0.20.2/xlsx-0.20.2.tgz"
	},
	"devDependencies": {
		"@antfu/eslint-config": "^4.3.0",
		"@antfu/utils": "^9.1.0",
		"@ruan-cat/commitlint-config": "^0.2.3",
		"@ruan-cat/domains": "^0.1.4",
		"@ruan-cat/utils": "^1.7.0",
		"@ruan-cat/vercel-deploy-tool": "^0.7.1",
		"@ruan-cat/vuepress-preset-config": "^0.1.19",
		"@types/eslint-config-prettier": "^6.11.3",
		"@types/lodash-es": "^4.17.12",
		"@types/node": "^22.13.4",
		"@types/qs": "^6.9.18",
		"@types/rollup-plugin-visualizer": "^4.2.4",
		"@types/splitpanes": "^2.2.6",
		"@vitejs/plugin-vue": "^5.2.1",
		"@vitest/ui": "^3.0.6",
		"commitizen": "^4.3.1",
		"consola": "^3.4.0",
		"cross-env": "^7.0.3",
		"cz-git": "^1.11.0",
		"del": "^8.0.0",
		"eslint": "^9.20.1",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-prettier": "^5.2.3",
		"eslint-plugin-vue": "^9.32.0",
		"jsdom": "^26.0.0",
		"prettier": "^3.5.1",
		"prettier-plugin-lint-md": "^1.0.1",
		"rollup-plugin-visualizer": "^5.14.0",
		"sass-embedded": "^1.85.0",
		"tsx": "^4.19.3",
		"type-plus": "^7.6.2",
		"typescript": "^5.7.3",
		"unplugin-auto-import": "^19.1.0",
		"unplugin-element-plus": "^0.9.1",
		"unplugin-icons": "^22.1.0",
		"unplugin-vue-components": "^28.4.0",
		"unplugin-vue-router": "^0.11.2",
		"vercel": "^41.1.4",
